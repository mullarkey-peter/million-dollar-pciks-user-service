scalar DateTime

type Query {
    userById(id: ID!): User
    userByUsername(username: String!): User
    userByEmail(email : String!): User
}

type Mutation {
    updateLastLogin(username: String!): User
    createOrUpdateUser(username: String!): User
}

type User {
    id: ID!
    username: String!
    email : String!
    registrationDate: DateTime
    lastLoginDate: DateTime
}